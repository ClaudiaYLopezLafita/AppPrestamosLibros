        <nav class="navbar navbar-expand-sm navbar-dark bg-dark mx-0">
            <div class="container-fluid">
                <div class="col-md-3">
                <a class="navbar-brand" href="/"><img src="/images/logo.png" id="logo" alt=""></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#mynavbar">
                    <span class="navbar-toggler-icon"></span>
                </button>
                </div>
                <div class="col-sm-1 collapse navbar-collapse" id="mynavbar">
                <ul class="navbar-nav me-auto">
                    <li class="nav-item">
                    <a class="nav-link text-white" href="javascript:void(0)">Inicio</a>
                    </li>
                    <li class="nav-item">
                    <a class="nav-link text-white" href="javascript:void(0)">Categorías</a>
                    </li>
                    <li class="nav-item">
                    <a class="nav-link text-white" href="javascript:void(0)">Sobre Nosotros</a>
                    </li>
                    <li class="nav-item">
                    <a class="nav-link text-white" href="/register">Registro</a>
                    </li>
                </ul>
                </div>
                <div class="col-sm">
                <form class="d-flex text-dark">
                    <button class="btn btn-login btn-outline-light rounded-pill me-2 text-white" type="button"><a href="/login" class="link-light">Login</a></button>
                    <input class="form-control me-2 rounded-pill" type="text" placeholder="Search">
                    <button class="btn btn-outline-light rounded-pill" type="button">Search</button>
                </form>
                </div>
            </div>
        </nav>
        <!-- END NAV-->
        <!-- START MAIN-->
        <div class="row justify-content-center py-lg-5 mainRegister">
            <div class="col-md-6 py-4 containerForm">
                <h1 class="mb-3 text-center"><%= title %> </h1>
                <form class="needs-validation px-3" method="post" action="/users" novalidate>
                    <div class="row justify-content-around my-2">
                        <div class="col-md-5">
                            <label for="username" class="form-label">Usuario</label>
                            <input type="text" class="form-control rounded-pill" name="username" id="username" placeholder="Usuario" value=""
                                required>
                        </div>
                        <div class="col-md-5">
                            <label for="password" class="form-label">Contraseña</label>
                            <input type="password" name="password" class="form-control rounded-pill" id="password" placeholder="Nombre" value=""
                                required>
                        </div>
                    </div>
                    <div class="row justify-content-around my-2">
                        <div class="col-md-5">
                            <label for="name" class="form-label">Nombre</label>
                            <input type="text" name="name" class="form-control rounded-pill" id="name" placeholder="Nombre" value=""
                                required>
                        </div>
                        <div class="col-md-5">
                            <label for="surname1" class="form-label">Apellido 1</label>
                            <input type="text" name="surname1" class="form-control rounded-pill" id="surname1" placeholder="Apellidos"
                                value="" required>
                        </div>
                    </div>
                    <div class="row justify-content-around my-2">
                        <div class="col-md-5">
                            <label for="surname2" class="form-label">Apellido 2</label>
                            <input type="text" name="surname2" class="form-control rounded-pill" id="surname2" placeholder="Apellidos"
                                value="" required>
                        </div>
                        <div class="col-md-5">
                            <label for="email" class="form-label">Email<span class="text-muted"></span></label>
                            <div class="input-group has-validation">
                                <span class="input-group-text rounded-pill"><i class="bi bi-envelope"></i></span>
                                <input type="email" name="email" class="form-control rounded-pill" id="email" placeholder="tu@ejemplo.com"
                                required>
                            </div>
                        </div>
                    </div>
                    <div class="row justify-content-around my-2">
                        <div class="col-md-5">
                            <label for="location" class="form-label">Dirección</label>
                            <div class="input-group has-validation">
                                <span class="input-group-text rounded-pill"><i class="bi bi-geo-alt"></i></span>
                                <input type="text" name="location" class="form-control rounded-pill" id="location" placeholder="Dirección">
                            </div>
                        </div>
                        <div class="col-md-5">
                            <label for="phone" class="form-label">Teléfono</label>
                            <div class="input-group has-validation">
                                <span class="input-group-text rounded-pill"><i class="bi bi-telephone"></i></span>
                                <input type="text" name="phone" class="form-control rounded-pill" id="phone" placeholder="Teléfono">
                            </div>
                        </div>
                    </div>
                    <hr class="my-4">
                    <div class="row justify-content-center my-2">
                        <div class="col-md-3">
                            <button class="btn btn-success btn-lg rounded-pill" type="submit">Register</button>
                        </div>
                        <div class="col-md-3">
                            <button class="btn btn-danger btn-lg rounded-pill" type="reset">Cancel</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <!-- START MAIN-->
        <!--START FOOTER-->
